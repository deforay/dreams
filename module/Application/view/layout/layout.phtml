    $countryLabDataReportingAccess = 'show';
    $countryLabDataExtractionAccess = 'show';
    $countryLabResultEmailAccess = 'show';
    $countryLabResultSMSAccess = 'show';
    $countryPrintLabLogbookAccess = 'show';
}else if($loginContainer->roleCode== 'LS'){
    $countryLabDataReportingAccess = 'show';
    $countryLabDataExtractionAccess = 'show';
    $countryLabResultEmailAccess = 'show';
    $countryLabResultSMSAccess = 'show';
}else if($loginContainer->roleCode== 'LDEO'){
    $countryLabDataReportingAccess = 'show';
    $countryLabDataExtractionAccess = 'show';
}
//set anc site sub-menu access by config values
$DRAccess = 'hide';
$PRAccess = 'hide';
if($loginContainer->roleCode== 'ANCSC'){
    if($loginContainer->hasDRAccess == 'yes'){
        $DRAccess = 'show';
    }
    if($loginContainer->hasPRAccess == 'yes'){
        $PRAccess = 'show';
    }
}
