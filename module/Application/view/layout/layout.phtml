use Zend\Session\Container;
$alertContainer = new Container('alert');
$loginContainer = new Container('user');
//set country main-menu access based on roles
$adminMenuAccess = 'hide';
$labMenuAccess = 'hide';
if($loginContainer->roleCode== 'CSC' || $loginContainer->roleCode== 'CC'){
    $adminMenuAccess = 'show';
    $labMenuAccess = 'show';
}else if($loginContainer->roleCode== 'LS' || $loginContainer->roleCode== 'LDEO'){
    $labMenuAccess = 'show';
}
//set country lab sub-menu access
$countryLabDataReportingAccess = 'hide';
$countryLabDataExtractionAccess = 'hide';
$countryLabResultEmailAccess = 'hide';
$countryLabResultSMSAccess = 'hide';
$countryPrintLabLogbookAccess = 'hide';
