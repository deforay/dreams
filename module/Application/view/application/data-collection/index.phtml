	    var pad = '000000';
	    var ctxt = '' + count;
	    $('#patientBarcodeId').val(countryCode + pad.substr(0, pad.length - ctxt.length) + ctxt);
	    $('label[for="patientBarcodeId"]').css("transform", "translateY(-140%)");
